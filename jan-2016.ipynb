    "time_vec = [10., 20., 100.]\n",
    "temp_vec = [303, 317, 303]\n",
    "\n",
    "for i in xrange(1000, 50000, 1000):\n",
    "    if i == 1000:\n",
    "        zinit = np.array([i, 0, total_HSP104, 5, 0])\n",
    "        x = run_simulation(Pabreg, zinit, temp_vec, time_vec, 0.01, .1)\n",
    "        #data1 = np.concatenate(([i], x[0][-1]))\n",
    "        data2 = np.concatenate(([i], [max(x[0][:, 2])]))\n",
    "    else:\n",
    "        zinit = np.array([i, 0, total_HSP104, 5, 0])\n",
    "        x = run_simulation(Pabreg, zinit, temp_vec, time_vec, 0.01, .1)\n",
    "        #xdata = np.concatenate(([i], x[0][-1]))\n",
    "        x2data = np.concatenate(([i], [max(x[0][:, 2])]))\n",
    "        #data1 = np.vstack((xdata, data1))\n",
    "        data2 = np.vstack((x2data, data2))\n",
    "        \n",
    "        \n",
    "fig, ax = plt.subplots(figsize = (6, 4))     \n",
    "ax.plot(data1[:, 0], data1[:, 3], c = \"firebrick\", linewidth=3)\n",
    "ax.plot(data2[:, 0], data2[:, 1], c='papayawhip', linewidth=3)\n",
    "ax.set_xlabel(\"Relative amount of Pab1\")\n",
    "ax.set_ylabel(\"Predicted HSP104 fold-change \\n at 20 min recovery\")\n",
    "ax.set_yscale('log')\n",
    "#ax.set_ylim(-1, 1)\n",
