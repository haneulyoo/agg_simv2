    "time_vec = [10., 20., 100.]\n",
    "temp_vec = [303, 317, 303]\n",
    "\n",
    "for i in xrange(1000, 50000, 1000):\n",
    "    if i == 1000:\n",
    "        zinit = np.array([i, 0, total_HSP104, 5, 0])\n",
    "        x = run_simulation(Pabreg, zinit, temp_vec, time_vec, 0.1, k6)\n",
    "        data1 = np.concatenate(([i], x[0][-1]))\n",
    "    else:\n",
    "        zinit = np.array([i, 0, total_HSP104, 5, 0])\n",
    "        x = run_simulation(Pabreg, zinit, temp_vec, time_vec, 0.1, k6)\n",
    "        xdata = np.concatenate(([i], x[0][-1]))\n",
    "        data1 = np.vstack((xdata, data1))\n",
    "        \n",
    "fig, ax = plt.subplots(figsize = (8, 6))     \n",
    "ax.plot(data1[:, 0]/13000., data1[:, 3]/100000. - 1, c = \"firebrick\", linewidth=4, label = 'Pab1-dependent model')\n",
    "ax.set_xlabel(\"Relative concentration of Pab1\")\n",
    "ax.set_ylabel(\"Predicted HSP104 \\n fold-change\")\n",
    "#ax.set_yscale('log')\n",
    "#ax.set_xscale('log')\n",
